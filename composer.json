{
	"name": "bit3/contao-dependency-container-inspector",
	"description": "Inspector for the dependency container for Contao open source cms.",
	"keywords": ["contao", "dependency container", "inspector"],
	"type": "contao-module",
	"license": "LGPL-3.0+",
	"authors": [
		{
			"name": "Tristan Lins",
			"email": "tristan.lins@bit3.de",
			"homepage": "http://bit3.de",
			"role": "Developer"
		}
	],
	"support": {
		"email": "info@bit3.de",
		"issues": "https://github.com/bit3/contao-dependency-container-inspector/issues",
		"wiki": "http://de.contaowiki.org/Dependency_Container",
		"irc": "irc://irc.freenode.org/contao.dev",
		"source": "https://github.com/bit3/contao-dependency-container-inspector"
	},
	"require": {
		"php": ">=5.3",
		"bit3/contao-dependency-container": "*",
		"contao-community-alliance/composer-installer": "*",
		"bit3/contao-twig": "~1"
	},
	"conflict": {
		"bit3/contao-dependency-container": "<1.2"
	},
	"autoload": {
		"psr-0": {
			"DependencyInjection\\Inspector": "src/"
		}
	},
	"extra": {
		"contao": {
			"sources": {
				"contao": "system/modules/dependency-container-inspector"
			}
		},
		"branch-alias": {
			"dev-master": "1.x-dev"
		}
	}
}
